      { "exclude": ["header", "footer", "@sectioning", "@heading"] }
    ]
  },

  "thead": {
    "implicitClosed": ["tbody", "tfoot"],
    "deprecatedAttributes": [
      "align",
      "background",
      "char",
      "charoff",
      "valign"
    ],
    "permittedContent": ["@script", "tr"]
  },

  "time": {
    "flow": true,
    "phrasing": true,
    "permittedContent": ["@phrasing"]
  },

  "title": {
    "metadata": true,
    "permittedContent": []
  },

  "tr": {
    "implicitClosed": ["tr"],
    "deprecatedAttributes": [
      "align",
      "background",
      "bgcolor",
      "char",
      "charoff",
      "valign"
    ],
    "permittedContent": ["@script", "td", "th"]
  },

  "track": {
    "void": true
  },

  "tt": {
    "deprecated": true
  },

  "u": {
    "flow": true,
    "phrasing": true,
    "permittedContent": ["@phrasing"]
  },

  "ul": {
    "flow": true,
    "deprecatedAttributes": ["compact", "type"],
    "permittedContent": ["@script", "li"]
  },

  "var": {
    "flow": true,
    "phrasing": true,
    "permittedContent": ["@phrasing"]
  },

  "video": {
    "flow": true,
    "phrasing": true,
    "embedded": true,
    "interactive": ["hasAttribute", "controls"],
    "transparent": true,
    "attributes": {
      "preload": ["", "none", "metadata", "auto"]
    },
    "permittedContent": ["@flow", "track", "source"],
    "permittedDescendants": [{ "exclude": ["audio", "video"] }],
    "permittedOrder": ["source", "track", "@flow"]
  },

  "wbr": {
    "flow": true,
    "phrasing": true,
    "void": true
  },

  "xmp": {
    "deprecated": true
  }
}
