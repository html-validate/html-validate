{
  "name": "html-validate",
  "version": "0.7.0",
  "description": "html linter",
  "keywords": [
    "html",
    "lint",
    "validator"
  ],
  "author": "David Sveningsson <ext@sidvind.com>",
  "license": "MIT",
  "homepage": "https://html-validate.sidvind.com",
  "repository": {
    "type": "git",
    "url": "https://git.sidvind.com/html-validate/html-validate.git"
  },
  "bugs": {
    "url": "https://git.sidvind.com/html-validate/html-validate/issues/new"
  },
  "main": "html-validate.js",
  "bin": {
    "html-validate": "html-validate.js"
  },
  "scripts": {
    "test": "grunt eslint mochaTest smoketest",
    "mocha": "nyc mocha --opts mocha.opts",
    "version": "./update-changelog.sh && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "deepmerge": "^2.1.0",
    "eslint": "^4.19.1",
    "glob": "^7.1.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "autoprefixer": "^8.5.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "bootstrap-sass": "^3.3.7",
    "canonical-path": "0.0.2",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "chai-subset": "^1.5.0",
    "columnify": "^1.5.4",
    "cssnano": "^3.10.0",
    "dgeni": "^0.4.9",
    "dgeni-packages": "^0.26.2",
    "eslint-config-sidvind": "^1.3.1",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-typescript": "^0.12.0",
    "font-awesome": "^4.7.0",
    "grunt": "^1.0.2",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-eslint": "^20.1.0",
    "grunt-mocha-test": "^0.13.2",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^2.1.0",
    "grunt-ts": "^6.0.0-beta.19",
    "highlight.js": "^9.12.0",
    "istanbul": "^0.4.5",
    "jquery": "^3.3.1",
    "load-grunt-tasks": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "serve-static": "^1.13.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.0.5",
    "tsconfig-paths": "^3.3.2",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.d.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  }
}
