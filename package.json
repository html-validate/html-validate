{
  "name": "html-validate",
  "version": "0.15.1",
  "description": "html linter",
  "keywords": [
    "html",
    "lint",
    "validator",
    "html-validate"
  ],
  "author": "David Sveningsson <ext@sidvind.com>",
  "license": "MIT",
  "homepage": "https://html-validate.sidvind.com",
  "repository": {
    "type": "git",
    "url": "https://git.sidvind.com/html-validate/html-validate.git"
  },
  "bugs": {
    "url": "https://git.sidvind.com/html-validate/html-validate/issues/new"
  },
  "main": "shim.js",
  "engines": {
    "node": ">=8.0"
  },
  "bin": {
    "html-validate": "bin/html-validate.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "grunt eslint",
    "version": "./scripts/update-changelog.sh && git add CHANGELOG.md",
    "preversion": "./scripts/validate-version.sh",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "acorn-walk": "^6.1.0",
    "deepmerge": "^2.2.1",
    "eslint": "^5.8.0",
    "espree": "^4.1.0",
    "glob": "^7.1.3",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.7",
    "@types/estree": "0.0.39",
    "@types/jest": "^23.3.9",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.2",
    "autoprefixer": "^9.3.1",
    "babelify": "^10.0.0",
    "bootstrap-sass": "^3.3.7",
    "canonical-path": "^1.0.0",
    "cssnano": "^4.1.7",
    "dgeni": "^0.4.10",
    "dgeni-packages": "^0.26.12",
    "eslint-config-sidvind": "^1.3.2",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-typescript": "^0.13.0",
    "font-awesome": "^4.7.0",
    "grunt": "^1.0.3",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-eslint": "^21.0.0",
    "grunt-exec": "^3.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^3.0.2",
    "grunt-ts": "^6.0.0-beta.21",
    "highlight.js": "^9.13.1",
    "jest": "^23.6.0",
    "jquery": "^3.3.1",
    "load-grunt-tasks": "^4.0.0",
    "sass": "^1.14.3",
    "serve-static": "^1.13.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "^20.1.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/**/index.ts",
      "!src/matchers.ts"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/docs"
    ],
    "snapshotSerializers": [
      "pretty-format/build/plugins/convert_ansi.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.+|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
