{
  "name": "html-validate",
  "version": "8.2.0",
  "description": "Offline html5 validator",
  "keywords": [
    "html",
    "lint",
    "validator",
    "html-validate"
  ],
  "homepage": "https://html-validate.org",
  "bugs": {
    "url": "https://gitlab.com/html-validate/html-validate/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/html-validate/html-validate.git"
  },
  "license": "MIT",
  "author": "David Sveningsson <ext@sidvind.com>",
  "sideEffects": [
    "./dist/*/jest.js"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/types/browser.d.ts",
        "require": "./dist/cjs/browser.js",
        "import": "./dist/es/browser.js"
      },
      "node": {
        "types": "./dist/types/index.d.ts",
        "require": "./dist/cjs/index.js",
        "import": "./dist/es/index.js"
      }
    },
    "./browser": {
      "types": "./dist/types/browser.d.ts",
      "require": "./dist/cjs/browser.js",
      "import": "./dist/es/browser.js"
    },
    "./node": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.js"
    },
    "./dist/cjs/*": "./dist/cjs/*",
    "./dist/es/*": "./dist/es/*",
    "./dist/schema/*": "./dist/schema/*",
    "./elements/html5": {
      "types": "./elements/html5.d.ts",
      "require": "./dist/cjs/html5.js",
      "import": "./dist/es/html5.js"
    },
    "./jest": {
      "types": "./dist/types/jest.d.ts",
      "require": "./dist/cjs/jest.js",
      "import": "./dist/es/jest.js"
    },
    "./package.json": "./package.json",
    "./test-utils": {
      "types": "./dist/types/test-utils.d.ts",
      "require": "./dist/cjs/test-utils.js",
      "import": "./dist/es/test-utils.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "browser": "dist/cjs/browser.js",
  "bin": {
    "html-validate": "bin/html-validate.js"
  },
  "files": [
    "bin",
    "dist",
    "elements",
    "browser.{js,d.ts}",
    "node.{js,d.ts}",
    "jest.{js,d.ts}",
    "test-utils.{js,d.ts}"
  ],
  "workspaces": [
    "docs"
  ],
  "scripts": {
    "prebuild": "run-s codegen prebuild:*",
    "build": "run-s build:cjs build:esm build:script",
    "postbuild": "bash scripts/pkg",
    "prebuild:tsc": "tsc",
    "build:cjs": "rollup --config rollup.cjs.config.mjs",
    "build:esm": "rollup --config rollup.esm.config.mjs",
    "build:script": "node build.mjs",
    "clean": "rm -rf dist public",
    "codegen": "node scripts/codegen.cjs",
    "compatibility": "scripts/compatibility.sh",
    "debug": "node --inspect ./node_modules/.bin/jest --runInBand --watch --no-coverage",
    "docs": "grunt docs",
    "postdocs": "tsc -b docs/examples",
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --cache --fix .",
    "htmlvalidate": "node ./bin/html-validate.js",
    "prepack": "npm run build && release-prepack package.json",
    "postpack": "release-postpack package.json",
    "prepare": "husky install && npm run codegen",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "grunt connect",
    "test": "jest --ci",
    "version": "scripts/version"
  },
  "commitlint": {
    "extends": "@html-validate",
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "api",
          "cli",
          "config",
          "context",
          "deps",
          "dom",
          "elements",
          "engine",
          "error",
          "eslint",
          "event",
          "formatters",
          "html5",
          "htmlvalidate",
          "jest",
          "lexer",
          "meta",
          "parser",
          "plugin",
          "prettier",
          "release",
          "reporter",
          "rules",
          "schema",
          "transform",
          "utils"
        ]
      ]
    }
  },
  "prettier": "@html-validate/prettier-config",
  "release": {
    "extends": "@html-validate/semantic-release-config"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/**/index.ts",
      "!src/index.ts",
      "!src/browser.ts",
      "!src/html5.ts",
      "!src/cli/html-validate.ts",
      "!src/jest/jest.ts",
      "!**/__fixtures__/**"
    ],
    "moduleNameMapper": {
      "^html-validate/browser$": "<rootDir>/src/browser.ts"
    },
    "preset": "@html-validate/jest-config",
    "roots": [
      "<rootDir>/docs",
      "<rootDir>/elements",
      "<rootDir>/src",
      "<rootDir>/tests"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.0",
    "@html-validate/stylish": "^4.1.0",
    "@sidvind/better-ajv-errors": "^2.0.0",
    "ajv": "^8.0.0",
    "deepmerge": "^4.2.0",
    "glob": "^10.0.0",
    "ignore": "^5.0.0",
    "kleur": "^4.1.0",
    "minimist": "^1.2.0",
    "prompts": "^2.0.0",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@html-validate/commitlint-config": "3.0.19",
    "@html-validate/eslint-config": "5.10.6",
    "@html-validate/eslint-config-jest": "5.10.0",
    "@html-validate/eslint-config-typescript": "5.10.6",
    "@html-validate/eslint-config-typescript-typeinfo": "5.10.6",
    "@html-validate/jest-config": "3.7.7",
    "@html-validate/prettier-config": "2.4.3",
    "@html-validate/release-scripts": "4.1.6",
    "@microsoft/api-extractor": "7.36.3",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-typescript": "11.1.2",
    "@rollup/plugin-virtual": "3.0.1",
    "@types/babar": "0.2.1",
    "@types/babel__code-frame": "7.0.3",
    "@types/jest": "29.5.3",
    "@types/minimist": "1.2.2",
    "@types/node": "16.18.39",
    "@types/prompts": "2.4.4",
    "@types/semver": "7.5.0",
    "@types/stream-buffers": "3.0.4",
    "babar": "0.2.3",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.6.2",
    "jest-diff": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "jest-snapshot": "29.6.2",
    "memfs": "4.2.0",
    "npm-pkg-lint": "2.0.0",
    "npm-run-all": "4.1.5",
    "rollup": "3.27.2",
    "stream-buffers": "3.0.2",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "peerDependencies": {
    "jest": "^27.1 || ^28.1.3 || ^29.0.3",
    "jest-diff": "^27.1 || ^28.1.3 || ^29.0.3",
    "jest-snapshot": "^27.1 || ^28.1.3 || ^29.0.3"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    },
    "jest-diff": {
      "optional": true
    },
    "jest-snapshot": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 16.0"
  },
  "renovate": {
    "extends": [
      "gitlab>html-validate/renovate-config"
    ]
  }
}
