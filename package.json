{
  "name": "html-validate",
  "version": "4.3.0",
  "description": "html linter",
  "keywords": [
    "html",
    "lint",
    "validator",
    "html-validate"
  ],
  "homepage": "https://html-validate.org",
  "bugs": {
    "url": "https://gitlab.com/html-validate/html-validate/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/html-validate/html-validate.git"
  },
  "license": "MIT",
  "author": "David Sveningsson <ext@sidvind.com>",
  "sideEffects": false,
  "main": "dist/main.js",
  "browser": "dist/browser.js",
  "bin": {
    "html-validate": "bin/html-validate.js"
  },
  "files": [
    "bin",
    "dist",
    "elements",
    "jest.{js,d.ts}",
    "test-utils.{js,d.ts}",
    "!**/*.snap",
    "!**/*.spec.d.ts",
    "!**/*.spec.js",
    "!**/*.spec.ts",
    "!**/__mocks__",
    "!dist/rules/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "build:docs": "grunt docs",
    "clean": "rm -rf dist public",
    "commitlint": "commitlint",
    "compatibility": "scripts/compatibility.sh",
    "debug": "node --inspect ./node_modules/.bin/jest --runInBand --watch --no-coverage",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "htmlvalidate": "./bin/html-validate.js",
    "prepare": "scripts/prepare.sh",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "semantic-release": "semantic-release",
    "start": "grunt connect",
    "test": "jest --ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "./scripts/pre-push"
    }
  },
  "commitlint": {
    "extends": [
      "@html-validate"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json,html,md,scss}": "prettier --write"
  },
  "prettier": "@html-validate/prettier-config",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/**/index.ts",
      "!src/main.ts",
      "!src/browser.ts",
      "!src/cli/html-validate.ts"
    ],
    "preset": "@html-validate/jest-config",
    "roots": [
      "<rootDir>/docs",
      "<rootDir>/elements",
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "snapshotSerializers": [
      "pretty-format/build/plugins/ConvertAnsi"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@html-validate/stylish": "1.0.0",
    "@sidvind/better-ajv-errors": "^0.8.0",
    "acorn-walk": "^8.0.0",
    "ajv": "^7.0.0",
    "chalk": "^4.0.0",
    "deepmerge": "^4.2.2",
    "espree": "^7.3.0",
    "glob": "^7.1.6",
    "json-merge-patch": "^1.0.1",
    "minimist": "^1.2.5",
    "prompts": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@commitlint/cli": "11.0.0",
    "@html-validate/commitlint-config": "1.2.0",
    "@html-validate/eslint-config": "3.1.0",
    "@html-validate/eslint-config-jest": "3.0.0",
    "@html-validate/eslint-config-typescript": "3.0.0",
    "@html-validate/jest-config": "1.2.3",
    "@html-validate/prettier-config": "1.1.0",
    "@html-validate/semantic-release-config": "1.2.5",
    "@lodder/grunt-postcss": "3.0.0",
    "@types/babel__code-frame": "7.0.2",
    "@types/estree": "0.0.46",
    "@types/glob": "7.1.3",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/json-merge-patch": "0.0.5",
    "@types/minimist": "1.2.1",
    "@types/node": "11.15.44",
    "@types/prompts": "2.0.9",
    "autoprefixer": "10.2.3",
    "babelify": "10.0.0",
    "bootstrap-sass": "3.4.1",
    "canonical-path": "1.0.0",
    "cssnano": "4.1.10",
    "dgeni": "0.4.13",
    "dgeni-front-matter": "2.0.3",
    "dgeni-packages": "0.28.4",
    "eslint": "7.18.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "font-awesome": "4.7.0",
    "front-matter": "4.0.2",
    "grunt": "1.3.0",
    "grunt-browserify": "5.3.0",
    "grunt-cli": "1.3.2",
    "grunt-contrib-connect": "3.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-sass": "3.1.0",
    "highlight.js": "10.5.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-diff": "26.6.2",
    "jquery": "3.5.1",
    "lint-staged": "10.5.3",
    "load-grunt-tasks": "5.1.0",
    "marked": "1.2.8",
    "minimatch": "3.0.4",
    "npm-pkg-lint": "1.3.0",
    "postcss": "8.2.4",
    "prettier": "2.2.1",
    "pretty-format": "26.6.2",
    "sass": "1.32.5",
    "semantic-release": "17.3.7",
    "serve-static": "1.14.1",
    "stringmap": "0.2.2",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "release": {
    "extends": "@html-validate/semantic-release-config"
  },
  "renovate": {
    "extends": [
      "@html-validate"
    ]
  }
}
