{
  "name": "html-validate",
  "version": "4.14.0",
  "description": "html linter",
  "keywords": [
    "html",
    "lint",
    "validator",
    "html-validate"
  ],
  "homepage": "https://html-validate.org",
  "bugs": {
    "url": "https://gitlab.com/html-validate/html-validate/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/html-validate/html-validate.git"
  },
  "license": "MIT",
  "author": "David Sveningsson <ext@sidvind.com>",
  "sideEffects": false,
  "main": "dist/main.js",
  "browser": "dist/browser.js",
  "bin": {
    "html-validate": "bin/html-validate.js"
  },
  "files": [
    "bin",
    "dist",
    "elements",
    "jest.{js,d.ts}",
    "test-utils.{js,d.ts}",
    "!**/*.map",
    "!**/*.snap",
    "!**/*.spec.d.ts",
    "!**/*.spec.js",
    "!**/*.spec.ts",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!dist/rules/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "build:docs": "grunt docs",
    "clean": "rm -rf dist public",
    "compatibility": "scripts/compatibility.sh",
    "debug": "node --inspect ./node_modules/.bin/jest --runInBand --watch --no-coverage",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "htmlvalidate": "node ./bin/html-validate.js",
    "prepare": "husky install scripts",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "grunt connect",
    "test": "jest --ci",
    "version": "scripts/version"
  },
  "commitlint": {
    "extends": [
      "@html-validate"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "cli",
          "config",
          "context",
          "deps",
          "dom",
          "elements",
          "engine",
          "error",
          "eslint",
          "event",
          "formatters",
          "html5",
          "htmlvalidate",
          "jest",
          "lexer",
          "meta",
          "parser",
          "plugin",
          "prettier",
          "release",
          "reporter",
          "rules",
          "schema",
          "transform",
          "utils"
        ]
      ]
    }
  },
  "lint-staged": {
    "*.{ts,js,json,html,md,scss,yml}": "prettier --write"
  },
  "prettier": "@html-validate/prettier-config",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/**/index.ts",
      "!src/main.ts",
      "!src/browser.ts",
      "!src/cli/html-validate.ts"
    ],
    "preset": "@html-validate/jest-config",
    "roots": [
      "<rootDir>/docs",
      "<rootDir>/elements",
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "snapshotSerializers": [
      "pretty-format/build/plugins/ConvertAnsi"
    ]
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.0",
    "@html-validate/stylish": "^1.0.0",
    "@sidvind/better-ajv-errors": "^0.9.0",
    "acorn-walk": "^8.0.0",
    "ajv": "^7.0.0",
    "deepmerge": "^4.2.0",
    "espree": "^7.3.0",
    "glob": "^7.1.0",
    "ignore": "^5.0.0",
    "json-merge-patch": "^1.0.0",
    "kleur": "^4.1.0",
    "minimist": "^1.2.0",
    "prompts": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.5",
    "@babel/preset-env": "7.14.5",
    "@commitlint/cli": "12.1.4",
    "@html-validate/commitlint-config": "1.3.1",
    "@html-validate/eslint-config": "4.4.1",
    "@html-validate/eslint-config-jest": "4.4.2",
    "@html-validate/eslint-config-typescript": "4.4.0",
    "@html-validate/jest-config": "1.2.10",
    "@html-validate/prettier-config": "1.1.0",
    "@html-validate/semantic-release-config": "1.2.15",
    "@lodder/grunt-postcss": "3.0.1",
    "@types/babar": "0.2.0",
    "@types/babel__code-frame": "7.0.2",
    "@types/estree": "0.0.47",
    "@types/glob": "7.1.3",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.23",
    "@types/json-merge-patch": "0.0.5",
    "@types/minimist": "1.2.1",
    "@types/node": "11.15.54",
    "@types/prompts": "2.0.13",
    "autoprefixer": "10.2.6",
    "babar": "0.2.0",
    "babelify": "10.0.0",
    "bootstrap-sass": "3.4.1",
    "canonical-path": "1.0.0",
    "cssnano": "5.0.6",
    "dgeni": "0.4.14",
    "dgeni-front-matter": "2.0.3",
    "dgeni-packages": "0.29.1",
    "eslint": "7.28.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.7.0",
    "font-awesome": "4.7.0",
    "front-matter": "4.0.2",
    "grunt": "1.4.1",
    "grunt-browserify": "6.0.0",
    "grunt-cli": "1.4.3",
    "grunt-contrib-connect": "3.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-sass": "3.1.0",
    "highlight.js": "11.0.1",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "jest-diff": "27.0.2",
    "jquery": "3.6.0",
    "lint-staged": "11.0.0",
    "load-grunt-tasks": "5.1.0",
    "marked": "2.0.7",
    "minimatch": "3.0.4",
    "npm-pkg-lint": "1.3.0",
    "postcss": "8.3.2",
    "prettier": "2.3.1",
    "pretty-format": "27.0.2",
    "sass": "1.34.1",
    "semantic-release": "17.4.3",
    "serve-static": "1.14.1",
    "stringmap": "0.2.2",
    "ts-jest": "27.0.3",
    "typescript": "4.3.2"
  },
  "peerDependencies": {
    "jest": "^24 || ^25 || ^26 || ^27"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 10.0"
  },
  "release": {
    "extends": "@html-validate/semantic-release-config"
  },
  "renovate": {
    "extends": [
      "@html-validate"
    ]
  }
}
