{
  "name": "html-validate",
  "version": "10.1.1",
  "description": "Offline HTML5 validator and linter",
  "keywords": [
    "html",
    "lint",
    "validator",
    "html-validate"
  ],
  "homepage": "https://html-validate.org",
  "bugs": {
    "url": "https://gitlab.com/html-validate/html-validate/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/html-validate/html-validate.git"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/html-validate"
    }
  ],
  "license": "MIT",
  "author": "David Sveningsson <ext@sidvind.com>",
  "sideEffects": [
    "./dist/*/jest.js"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/types/browser.d.ts",
        "require": "./dist/cjs/browser.js",
        "import": "./dist/esm/browser.js"
      },
      "node": {
        "types": "./dist/types/index.d.ts",
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      }
    },
    "./browser": {
      "types": "./dist/types/browser.d.ts",
      "require": "./dist/cjs/browser.js",
      "import": "./dist/esm/browser.js"
    },
    "./node": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./dist/schema/*": "./dist/schema/*",
    "./elements/html5": {
      "types": "./dist/types/html5.d.ts",
      "require": "./dist/cjs/html5.js",
      "import": "./dist/esm/html5.js"
    },
    "./jest": {
      "types": "./dist/types/jest.d.ts",
      "require": "./dist/cjs/jest.js",
      "import": "./dist/esm/jest.js"
    },
    "./package.json": "./package.json",
    "./test-utils": {
      "types": "./dist/types/test-utils.d.ts",
      "require": "./dist/cjs/test-utils.js",
      "import": "./dist/esm/test-utils.js"
    },
    "./vitest": {
      "types": "./dist/types/vitest.d.ts",
      "import": "./dist/esm/vitest.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/cjs/browser.js",
  "bin": {
    "html-validate": "bin/html-validate.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "workspaces": [
    "docs",
    "internal/*",
    "tests/integration/cjs",
    "tests/integration/cjs-cli",
    "tests/integration/esm",
    "tests/integration/esm-cli",
    "tests/vitest"
  ],
  "scripts": {
    "prebuild": "run-s codegen && tsc",
    "build": "run-s build:cjs build:esm build:script",
    "postbuild": "npm run --workspaces --if-present build",
    "build:cjs": "rollup --config rollup.cjs.config.mjs",
    "build:esm": "rollup --config rollup.esm.config.mjs",
    "build:script": "node build.mjs",
    "clean": "rm -rf dist public",
    "codegen": "node scripts/codegen.cjs",
    "compatibility": "scripts/compatibility.sh",
    "debug": "node --inspect ./node_modules/.bin/jest --runInBand --watch --no-coverage",
    "docs": "node docs/build.js",
    "postdocs": "tsc -b docs/examples",
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --cache --fix .",
    "htmlvalidate": "node ./bin/html-validate.mjs",
    "prepack": "release-prepack --bundle",
    "postpack": "release-postpack",
    "prepare": "husky && npm run codegen",
    "prepublishOnly": "release-prepublish --bundle",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postpublish": "release-postpublish",
    "start": "node docs/server.js",
    "test": "jest --ci",
    "version": "scripts/version",
    "postversion": "npm run build"
  },
  "commitlint": {
    "extends": "@html-validate",
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "api",
          "cli",
          "config",
          "context",
          "docs",
          "deps",
          "dom",
          "elements",
          "engine",
          "error",
          "eslint",
          "event",
          "formatters",
          "html5",
          "htmlvalidate",
          "jest",
          "lexer",
          "meta",
          "parser",
          "plugin",
          "prettier",
          "release",
          "reporter",
          "rules",
          "schema",
          "transform",
          "types",
          "utils",
          "vitest"
        ]
      ]
    }
  },
  "prettier": "@html-validate/prettier-config",
  "release": {
    "extends": "@html-validate/semantic-release-config"
  },
  "jest": {
    "collectCoverageFrom": [
      "internal/*/src/**/*.ts",
      "src/**/*.ts",
      "!**/*.spec.ts",
      "!**/index.ts",
      "!src/browser.ts",
      "!src/html5.ts",
      "!src/htmlvalidate.browser.ts",
      "!src/cli/have-import-meta-resolve.ts",
      "!src/cli/html-validate.ts",
      "!src/jest/jest.ts",
      "!src/jest/worker/**",
      "!src/vitest/vitest.ts",
      "!src/vitest/augmentation.ts",
      "!src/utils/compatibility-check.browser.ts",
      "!src/utils/compatibility-check.nodejs.ts",
      "!**/*.d.ts",
      "!**/__fixtures__/**"
    ],
    "globalSetup": "<rootDir>/jest.global.js",
    "moduleNameMapper": {
      "^html-validate/browser$": "<rootDir>/src/browser.ts"
    },
    "preset": "@html-validate/jest-config",
    "roots": [
      "<rootDir>/docs",
      "<rootDir>/internal/test-utils/src",
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironmentOptions": {
      "globalsCleanup": "on"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/tests/vitest/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "jest.setup.ts"
    ]
  },
  "dependencies": {
    "@html-validate/stylish": "^4.1.0",
    "@sidvind/better-ajv-errors": "4.0.0",
    "ajv": "^8.0.0",
    "deepmerge": "4.3.1",
    "glob": "^11.0.0",
    "ignore": "7.0.5",
    "kleur": "^4.1.0",
    "minimist": "^1.2.0",
    "prompts": "^2.0.0",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@babel/code-frame": "7.27.1",
    "@html-validate/commitlint-config": "3.6.1",
    "@html-validate/eslint-config": "6.9.0",
    "@html-validate/eslint-config-jest": "6.9.0",
    "@html-validate/eslint-config-typescript": "6.9.0",
    "@html-validate/eslint-config-typescript-typeinfo": "6.9.0",
    "@html-validate/jest-config": "3.13.0",
    "@html-validate/prettier-config": "3.1.4",
    "@html-validate/release-scripts": "6.10.0",
    "@html-validate/rollup-plugin-legacy": "file:internal/rollup-plugin-legacy",
    "@html-validate/rollup-plugin-packagejson": "file:internal/rollup-plugin-packagejson",
    "@microsoft/api-extractor": "7.53.0",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.2",
    "@rollup/plugin-replace": "6.0.2",
    "@rollup/plugin-virtual": "3.0.2",
    "@types/babar": "0.2.4",
    "@types/babel__code-frame": "7.0.6",
    "@types/cli-table": "0.3.4",
    "@types/jest": "30.0.0",
    "@types/minimist": "1.2.5",
    "@types/node": "20.19.19",
    "@types/prompts": "2.4.9",
    "@types/semver": "7.7.1",
    "@types/stream-buffers": "3.0.7",
    "babar": "0.2.3",
    "cli-table": "0.3.11",
    "esbuild": "0.25.10",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jest": "30.2.0",
    "jest-diff": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-snapshot": "30.2.0",
    "jsdom": "27.0.0",
    "magic-string": "0.30.19",
    "memfs": "4.49.0",
    "npm-pkg-lint": "4.1.1",
    "npm-run-all2": "8.0.4",
    "rollup": "4.52.4",
    "rollup-plugin-esbuild": "6.2.1",
    "stream-buffers": "3.0.3",
    "ts-jest": "29.4.4",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "jest": "^28.1.3 || ^29.0.3 || ^30.0.0",
    "jest-diff": "^28.1.3 || ^29.0.3 || ^30.0.0",
    "jest-snapshot": "^28.1.3 || ^29.0.3 || ^30.0.0",
    "vitest": "^1.0.0 || ^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    },
    "jest-diff": {
      "optional": true
    },
    "jest-snapshot": {
      "optional": true
    },
    "vitest": {
      "optional": true
    }
  },
  "engines": {
    "node": "^20.19.0 || >= 22.12.0"
  },
  "externalDependencies": [
    "@babel/code-frame",
    "@html-validate/stylish",
    "@sidvind/better-ajv-errors",
    "ajv",
    "glob",
    "kleur",
    "minimist",
    "prompts",
    "semver"
  ]
}
